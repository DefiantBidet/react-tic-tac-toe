{
  "name": "react-tic-tac-toe",
  "version": "0.0.1",
  "description": "coding test to create tic tac toe but in react",
  "main": "src/ts/index.tsx",
  "scripts": {
    "clean": "rm -rf dist/*",
    "lint": "eslint -c ./lib/eslint/eslintrc.json ./src/ts/*",
    "lint:fix": "eslint -c ./lib/eslint/eslintrc.json ./src/ts/* --fix",
    "lint:quiet": "eslint -c ./lib/eslint/eslintrc.json ./src/ts/* --quiet",
    "start": "npm run clean && webpack-dev-server --mode development --config ./lib/webpack/webpack.config.js",
    "build": "npm run clean && webpack --mode production --config ./lib/webpack/prod.webpack.config.js",
    "test": "NODE_ENV=test jest",
    "test:coverage": "NODE_ENV=test jest --coverage --notify",
    "test:watch": "NODE_ENV=test jest --watchAll"
  },
  "author": "DefiantBidet <James Dean>",
  "license": "MIT",
  "dependencies": {
    "grommet": "2.32.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "styled-components": "5.3.11"
  },
  "devDependencies": {
    "@babel/core": "7.22.5",
    "@babel/preset-env": "7.22.5",
    "@babel/preset-react": "7.22.5",
    "@babel/preset-typescript": "7.22.5",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/jest": "29.5.2",
    "@types/node": "20.3.1",
    "@types/react": "18.2.12",
    "@types/react-dom": "18.2.5",
    "@types/webpack": "5.28.1",
    "babel-loader": "9.1.2",
    "babel-plugin-module-resolver": "5.0.0",
    "eslint": "8.42.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "html-loader": "4.2.0",
    "html-webpack-plugin": "5.5.3",
    "jest": "29.5.0",
    "jest-environment-jsdom": "29.5.0",
    "node-notifier": "10.0.1",
    "prettier": "2.8.8",
    "ts-jest": "29.1.0",
    "ts-loader": "9.4.3",
    "typescript": "5.1.3",
    "webpack": "5.86.0",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "4.15.1"
  }
}
